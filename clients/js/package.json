  },
  "dependencies": {
    "@trezoaplex-foundation/tpl-essentials": "^0.5.2",
    "@trezoaplex-foundation/tpl-token-metadata": "3.0.0-alpha.13"
  },
  "devDependencies": {
    "@ava/typescript": "^3.0.1",
    "@trezoaplex-foundation/umi": "^0.6.0",
    "@trezoaplex-foundation/umi-bundle-tests": "^0.6.0",
    "@trezoaplex-foundation/tpl-token-auth-rules": "^1.2.0",
    "@trezoa/web3.js": "^1.73.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.46.1",
    "ava": "^5.1.0",
    "eslint": "^8.0.1",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "typedoc": "^0.23.16",
    "typedoc-plugin-expand-object-like-types": "^0.1.2",
    "typedoc-plugin-missing-exports": "^1.0.0",
    "typescript": "^4.6.2",
    "vercel": "^28.16.0"
